{
  "functions": [
    {
      "source": "mailgun",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": ["npm --prefix \"$RESOURCE_DIR\" run build"]
    }
  ],
  "hosting": {
    "rewrites": [
      {
        "source": "/mailgun/webhooks/**",
        "function": "mailgun__webhooks"
      }
    ]
  }
}
